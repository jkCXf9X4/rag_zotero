# Required for OpenAI embeddings
OPENAI_API_KEY=

# Optional: evaluate retrieval relevance via OpenRouter (used by `rag-zotero query --eval`)
OPENROUTER_API_KEY=
OPENROUTER_EVAL_MODEL=openai/gpt-4o-mini

# Optional overrides
OPENAI_EMBED_MODEL=text-embedding-3-small
CHROMA_DIR=./data/chroma
CHROMA_COLLECTION=zotero

# Indexing defaults
CHUNK_SIZE=1200
CHUNK_OVERLAP=200
